{
  "name": "apollo-server-starter",
  "version": "1.0.0",
  "description": "Starter for Apollo-server with sequelize ORM and PostgreSql database",
  "main": "index.js",
  "author": "Sulaiman Sanusi ",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "babel src --out-dir dist",
    "dev": "nodemon --exec yarn restart",
    "restart": "rimraf dist && yarn build && yarn start",
    "start": "node dist/index.js",
    "create:db": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.2",
    "prettier": "1.18.2",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@hugodf/sequelize-cli": "^5.4.0",
    "@sendgrid/mail": "^6.4.0",
    "apollo-errors": "^1.9.0",
    "apollo-server": "^2.9.3",
    "bcrypt": "^3.0.6",
    "graphql": "^14.5.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "nodemailer": "^6.4.16",
    "nodemailer-sendgrid": "^1.0.3",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.18.1"
  }
}
